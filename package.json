{
  "name": "poc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "test": "npx jest --collect-coverage --silent",
    "test:debug": "npx jest --collect-coverage",
    "lint": "eslint .",
    "security": "npx snyk test",
    "local:setup": "npm run migrations && npm run migrations:test && npm run seeds && npm run seeds:test && npx snyk auth",
    "migrations": "npx knex migrate:latest",
    "migrations:test": "npx knex migrate:latest --env test",
    "seeds": "npx knex seed:run",
    "seeds:test": "npx knex seed:run --env test"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.21.5",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.1",
    "snyk": "^1.381.2",
    "sqlite3": "^5.0.0",
    "superagent": "^6.0.0",
    "supertest": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ]
  }
}
